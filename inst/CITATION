citHeader("To cite macleish in publications use:")

citEntry(entry = "Manual",
  title        = "Weather and spatial data from the MacLeish field station",
  author       = personList(as.person("Benjamin S. Baumer"),
                   as.person("Rose Goueth"), 
                   as.person("Wencong Li"), 
                   as.person("Weijia Zhang")),
  year         = "2016",

  textVersion  =
  paste("Benjamin S. Baumer, Rose Goueth, Wencong Li, Weijia Zhang (2016).",
        "Weather and spatial data from the MacLeish field station.")
)